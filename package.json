{
  "name": "faq-chrome-extension",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "copy-background": "copyfiles src/background.js -u 1 background.js",
    "copy": "copyfiles manifest.json dist/ && npm run copy-icons-dev",
    "copy-prod": "copyfiles manifest.json gen/ && npm run copy-icons-prod",
    "copy-icons-prod": "copyfiles icons/* gen/",
    "copy-icons-dev": "copyfiles icons/* dist/",
    "cleandir": "rm -rf dist/*",
    "clean": "npm run cleandir && npm run copy",
    "build": "webpack --config=build-configs/webpack.config.dev.js && npm run copy",
    "watch": "webpack --watch --config=build-configs/webpack.config.dev.js",
    "prod": "webpack --config=build-configs/webpack.config.prod.js && npm run copy-prod"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "babel-loader": "^8.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "copyfiles": "^2.3.0",
    "css-loader": "^6.7.3",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "sass": "^1.26.7",
    "sass-loader": "^13.2.0",
    "terser-webpack-plugin": "^3.0.3",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "extract-domain": "^2.2.1",
    "firebase": "^7.14.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
